{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-underscore-dangle": ["error", { "enforceInMethodNames": false }],
    "import/prefer-default-export": "off",
    "prettier/prettier": "error"
  }
}
